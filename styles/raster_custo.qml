<!DOCTYPE qgis PUBLIC 'http://mrcc.com/qgis.dtd' 'SYSTEM'>
<qgis>
  <pipe>
    <rasterrenderer type="singlebandpseudocolor" band="1" classificationMin="0" classificationMax="255">
      <rastershader>
        <colorrampshader colorRampType="INTERPOLATED">
          <item alpha="255" value="0" label="Min" color="#1b006e"/>
          <item alpha="255" value="128" label="Mid" color="#0037ff"/>
          <item alpha="255" value="255" label="Max" color="#00ffff"/>
        </colorrampshader>
      </rastershader>
    </rasterrenderer>
  </pipe>
</qgis>
